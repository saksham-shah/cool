extract Console

Car = class (yearModel, make) {
    init this.speed = new Number(0)

    getYearModel = () => yearModel
    getMake = () => make
    getSpeed = () => speed

    accelerate = () => this.speed += 5
    brake = () => this.speed -= 5
    
}

myCar = new Car(2001, "cool car")

while (-1 + i += 1) < 5 {
    myCar.accelerate()
    print(myCar.getSpeed())
}

while (-1 + j += 1) < 5 {
    myCar.brake()
    print(myCar.getSpeed())
}


