
extract Console

classes = []

Car = class (price) {
    static wheels = 4

    static horn = () => print("HOOOOOOOOOORN!!!")

    getPrice = () => print("I cost " + this.price + " moneys.")
}

Car.horn()

test = () => Car

myCar = new test()(1)

print(myCar)

classes += Car
classes += Car
classes += Car

nextCar = new classes[1](500)

myCar.getPrice()

nextCar.getPrice()

print(classes);

